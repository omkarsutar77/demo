sudo yum install nginx php mariadb105-server -y
sudo yum install php8.3-mysqlnd.x86_64 -y         //php-mysql connector
start all services nginx php-fpm mariadb
cd /usr/share/nginx/html/     //in this path check index.html and index.php
sudo mysql
ALTER USER root@localhost identified by "pass123";   //create passwword and exit
sudo mysql -u root -p       //login in mysql and create a table using mysql.txt
create a form.html and submit.html    
restart all services 
